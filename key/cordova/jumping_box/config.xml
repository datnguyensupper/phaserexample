<?xml version='1.0' encoding='utf-8'?>
<widget id="com.donick.jumpingbox" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JumpingBox</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dat.nguyen.supper@gmail.com" href="http://cordova.io">donick</author>
    <content src="index.html" />
    <preference name="orientation" value="landscape" />
    <plugin name="cordova-plugin-crosswalk-webview" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-statusbar" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.0.0" />
    <plugin name="cordova-plugin-nativeaudio" spec="https://github.com/didlich/cordova-plugin-nativeaudio#xml-invalid-chars" />
    <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
        <variable name="APP_ID" value="254723221616616" />
        <variable name="APP_NAME" value="JumpingBox" />
    </plugin>
</widget>
